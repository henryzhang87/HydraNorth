    <foxml:datastreamVersion CREATED="2010-11-12T17:32:56.129Z" FORMAT_URI="http://www.openarchives.org/OAI/2.0/oai_dc/" ID="DC.1" LABEL="Monaco Glacier, Spitsbergen - photograph" MIMETYPE="text/xml" SIZE="1083">
      <foxml:xmlContent>
        <oai_dc:dc xmlns:oai_dc="http://www.openarchives.org/OAI/2.0/oai_dc/" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:ualterms="http://terms.lib ` rary/ualberta.ca" xsi:schemaLocation="http://www.openarchives.org/OAI/2.0/oai_dc/ http://www.openarchives.org/OAI/2.0/oai_dc.xsd">
          <dc:title>Monaco Glacier, Spitsbergen - photograph</dc:title>
          <dc:creator>Seale, Linda N&gt;</dc:creator>
          <dc:subject>Monaco Glacier</dc:subject>
          <dc:subject>Spitsbergen, Norway</dc:subject>
          <dc:subject>Seabirds</dc:subject>
          <dc:subject>Turbulence</dc:subject>
          <dc:subject>Mixing of fresh and salt water</dc:subject>
          <dc:description>The Monaco Glacier reaches sea level on the north coast of Spitsbergen, Norway. The fresh water flowing under the glacier mixes with sea water and creates the turbulence visible in the photograph. This brings sea life to the surface and attracts thousands of sea birds to the area. August 2008</dc:description>
          <dc:date>2010/11/12</dc:date>
          <dc:type>Learning Object</dc:type>
          <dc:language>eng</dc:language>
          <dc:coverage xsi:nil="true"/>
          <dc:coverage>Monaco Glacier at sea level, north coast of Spitsbergen, Norway</dc:coverage>
        </oai_dc:dc>
      </foxml:xmlContent>
