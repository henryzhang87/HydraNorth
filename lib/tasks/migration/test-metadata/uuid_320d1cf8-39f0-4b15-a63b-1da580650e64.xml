      <foxml:contentLocation REF="http://era.library.ualberta.ca:8180/fedora/get/uuid:320d1cf8-39f0-4b15-a63b-1da580650e64/DS1/2014-04-24T20:02:21.992Z" TYPE="INTERNAL_ID"/>
