    <foxml:datastreamVersion CREATED="2011-05-25T20:13:12.653Z" FORMAT_URI="http://www.openarchives.org/OAI/2.0/oai_dc/" ID="DC.1" LABEL="Port Foster, Deception Island -- photograph" MIMETYPE="text/xml">
      <foxml:xmlContent>
        <oai_dc:dc xmlns:oai_dc="http://www.openarchives.org/OAI/2.0/oai_dc/" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:ualterms="http://terms.lib ` rary/ualberta.ca" xsi:schemaLocation="http://www.openarchives.org/OAI/2.0/oai_dc/ http://www.openarchives.org/OAI/2.0/oai_dc.xsd">
          <dc:identifier>http://hdl.handle.net/10402/era.23313</dc:identifier>
          <dc:title>Port Foster, Deception Island -- photograph</dc:title>
          <dc:creator>Seale, Linda N.</dc:creator>
          <dc:subject>Port Foster</dc:subject>
          <dc:subject>Deception Island</dc:subject>
          <dc:subject>Antarctic Peninsula</dc:subject>
          <dc:subject>Whaling</dc:subject>
          <dc:subject>Whaling ships</dc:subject>
          <dc:description>Area known as 'Port Foster' at Deception Island off the Antarctic Peninsula, where whaling ships anchored because it is furthest from the entrance to the bay and is most sheltered. Deception Island is a drowned caldera (volcanic crater) covered in black volcanic sand from the last eruption in 1970.  Photographed January 2004</dc:description>
          <dc:date>2004/01</dc:date>
          <dc:type>Research Material</dc:type>
          <dc:format>image/pjpeg</dc:format>
          <dc:language>eng</dc:language>
          <dc:coverage xsi:nil="true"/>
          <dc:coverage>Deception Island</dc:coverage>
        </oai_dc:dc>
      </foxml:xmlContent>
    </foxml:datastreamVersion>
